<%= simple_form_for @car do |f| %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :color %>
  <%= f.input :location %>
  <%= f.input :engine_size %>
  <%= f.input :plate %>
  <%= f.input :price, label: "Price per day" %>
  <%= f.input :photos, as: :file, input_html: { multiple:true } %>
  <%= f.button :submit %>
<% end %>
